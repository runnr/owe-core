{
	"env": {
		"node": true,
		"es6": true
	},
	"rules": {
		"no-redeclare": 2,
		"no-loop-func": 0,
		"new-cap": 1,
		"strict": [2, "global"],
		"no-func-assign": 1,
		"no-inner-declarations": 2,
		"no-obj-calls": 2,
		"no-var": 2,
		"no-const-assign": 2,
		"no-ex-assign": 2,
		"no-with": 1,
		"no-mixed-spaces-and-tabs": 1,
		"no-multiple-empty-lines": [1, {
			"max": 1
		}],
		"no-multi-str": 1,
		"no-lonely-if": 1,
		"no-undef-init": 1,
		"no-undef": 2,
		"no-unused-vars": 1,
		"no-delete-var": 2,
		"no-self-compare": 1,
		"no-multi-spaces": 1,
		"no-unneeded-ternary": 1,
		"no-extra-boolean-cast": 1,
		"no-extra-semi": 1,
		"no-extra-parens": 1,
		"no-array-constructor": 1,
		"no-new-object": 1,
		"no-dupe-keys": 2,
		"no-duplicate-case": 2,
		"no-dupe-args": 2,
		"no-irregular-whitespace": 2,
		"no-sparse-arrays": 2,
		"no-cond-assign": [1, "except-parens"],
		"operator-linebreak": [1, "before"],
		"space-after-keywords": [1, "never"],
		"space-before-keywords": [1, "always"],
		"key-spacing": [1, {
			"beforeColon": false,
			"afterColon": true
		}],
		"space-unary-ops": 1,
		"semi-spacing": [1, {
			"before": false,
			"after": true
		}],
		"no-spaced-func": 1,
		"spaced-comment": [1, "always", {
			"block": {
				"exceptions": ["*"],
				"markers": ["*package"]
			}
		}],
		"space-before-function-paren": [1, "never"],
		"space-in-parens": [1, "never"],
		"comma-dangle": [1, "never"],
		"comma-style": [1, "last"],
		"no-trailing-spaces": 1,
		"no-underscore-dangle": 1,
		"camelcase": [1, {
			"properties": "always"
		}],
		"brace-style": [1, "stroustrup"],
		"newline-after-var": [1, "always"],
		"eol-last": 1,
		"lines-around-comment": [1, {
			"beforeBlockComment": true,
			"beforeLineComment": false,
			"afterBlockComment": false,
			"afterLineComment": false
		}],
		"semi": [1, "always"],
		"space-infix-ops": 1,
		"space-return-throw-case": 1,
		"space-before-blocks": [1, "always"],
		"consistent-this": [1, "that"],
		"indent": [1, "tab", {
			"SwitchCase": 1
		}],
		"linebreak-style": [1, "unix"],
		"quotes": [1, "double", "avoid-escape"],
		"prefer-const": 1,
		"prefer-arrow-callback": 1,
		"prefer-template": 1,
		"arrow-parens": [1, "as-needed"],
		"arrow-spacing": 1,
		"object-shorthand": 1,
		"generator-star-spacing": [1, "after"],
		"no-unexpected-multiline": 1,
		"no-unreachable": 1,
		"use-isnan": 2,
		"valid-typeof": 2,
		"valid-jsdoc": 2,
		"no-negated-in-lhs": 2,
		"dot-location": [1, "property"],
		"dot-notation": [1, {
			"allowKeywords": true,
			"allowPattern": "^[a-z]+(_[a-z]+)+$"
		}],
		"eqeqeq": [2, "allow-null"],
	}
}
